<div class="jumbotron row">
    <div class="text-center form-group">
        <h1>I'm looking for a clinic in</h1>
        <input type="location" class="form-control" id="location" placeholder="Enter zip code or city">
    </div>
</div>
<div class="jumbotron row">
    <div class="text-center">
       <h1>Have you visited a clinic recently?</h1>
        <%= link_to 'Write a Review', new_review_path, :class => 'btn btn-raised btn-success' %>
    </div>
</div>

<h1>Recent Reviews</h1>
<% if current_user %>
    <%= current_user.email %>
<% end %>
