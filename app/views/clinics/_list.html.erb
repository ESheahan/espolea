<div id="filterrific_results">

  <div>
    <%=  page_entries_info @clinics %>
  </div>

  <table class="table-striped table-hover col-lg-12">
    <tr>
      <th>Name</th>
      <th>Phone number</th>
      <th>Email</th>
      <th>Website</th>
      <th>State</th>
      <th>Municipality</th>
      <th colspan="3"></th>
    </tr>

    <% @clinics.each do |clinic| %>
      <tr>
        <td><%= link_to(clinic.name, clinic_path(clinic)) %></td>
        <td><%= clinic.phone_number %></td>
        <td><%= clinic.email %></td>
        <td><%= clinic.website %></td>
        <td><%= clinic.state %></td>
        <td><%= clinic.municipality %></td>
        <td><%= link_to 'Show', clinic %></td>
        <td><%= link_to 'Edit', edit_clinic_path(clinic) %></td>
        <td><%= link_to 'Destroy', clinic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
</table>
</div>
<%=  will_paginate @clinics %>